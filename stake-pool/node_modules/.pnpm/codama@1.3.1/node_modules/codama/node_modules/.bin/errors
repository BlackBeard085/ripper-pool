#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/BlackBeard/ripperpool/stake-pool/node_modules/.pnpm/@codama+errors@1.3.1/node_modules/@codama/errors/bin/node_modules:/home/BlackBeard/ripperpool/stake-pool/node_modules/.pnpm/@codama+errors@1.3.1/node_modules/@codama/errors/node_modules:/home/BlackBeard/ripperpool/stake-pool/node_modules/.pnpm/@codama+errors@1.3.1/node_modules/@codama/node_modules:/home/BlackBeard/ripperpool/stake-pool/node_modules/.pnpm/@codama+errors@1.3.1/node_modules:/home/BlackBeard/ripperpool/stake-pool/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/BlackBeard/ripperpool/stake-pool/node_modules/.pnpm/@codama+errors@1.3.1/node_modules/@codama/errors/bin/node_modules:/home/BlackBeard/ripperpool/stake-pool/node_modules/.pnpm/@codama+errors@1.3.1/node_modules/@codama/errors/node_modules:/home/BlackBeard/ripperpool/stake-pool/node_modules/.pnpm/@codama+errors@1.3.1/node_modules/@codama/node_modules:/home/BlackBeard/ripperpool/stake-pool/node_modules/.pnpm/@codama+errors@1.3.1/node_modules:/home/BlackBeard/ripperpool/stake-pool/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@codama+errors@1.3.1/node_modules/@codama/errors/bin/cli.cjs" "$@"
else
  exec node  "$basedir/../../../../../@codama+errors@1.3.1/node_modules/@codama/errors/bin/cli.cjs" "$@"
fi
